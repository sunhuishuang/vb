//Top Top({‘isIndex’:true}) // isIndex的意思是判断首页是否首页打开
var Top = function (option) {
    for (var i in option) {
        this[i] = option[i];
    }


    this.init();
}
Top.prototype.init = function () {
    this.selectInit();
    this.bannerInit();
    this.trigger();
}

//全部商品选项卡,初始化
Top.prototype.selectInit = function () {
    var xthis = this;
    var hoverEvent = function(){
    	var topSelect = $('.top-select');
        $(".top-select-cont").removeAttr("style");
        topSelect.find('.categoryBoxTabUl li').eq(0).addClass('active');
        topSelect.find('.categoryBoxTabList>div').eq(0).show();
    }
    if($('#index').length > 0){
    	hoverEvent();
    }else{
    	if (!this.isIndex) {
	        hoverEvent();
	    }
    };
    
    $('.categoryBoxTabUl').on('click','li',function(){
    	var el = $(this);
    	var category = el.data('category');
    	el.addClass('active').siblings().removeClass('active');
    	$('.'+category).show().siblings().hide();
    })
}

//banner初始化
Top.prototype.bannerInit = function () {
    if($.cookie("topAdClosed")){
        $('.top-banner').hide();
    }else{
        if($('.top-banner').hasClass('none')){
            $('.top-banner').hide();
        }else{
            $('.top-banner').show();}
    }
}
Top.prototype.trigger = function () {
    if (!$(".top-banner")) {
        EventLoop.trigger('topAdLoaded');
    }
    $('.top-banner-close').click(function () {
        $.cookie("topAdClosed",'true');
        $('.top-banner').hide();
        EventLoop.trigger('topAdClosed', $('.top-banner').height());
    })
}
Top.prototype.lazy = function (dom) {
    if (dom.find('.te-lazy-100').eq(0).attr('data-original') == dom.find('.te-lazy-100').eq(0).attr('src')) return false;
    $("body,html").trigger("scroll");
}


var Foot = function () {
    this.init();
    this.popTipsInit();
    this.lazyloadInit();

}
Foot.prototype.init = function () {
}
//弹出层初始化
Foot.prototype.popTipsInit = function () {
    $(".com-tips-box").parent('').hover(function () {
        $(this).find(".com-tips-box").show();
    }, function () {
        $(this).find(".com-tips-box").hide();
    })
}
Foot.prototype.lazyloadInit = function(){

        $(function(){
            imgLazyLoad('fadeIn');
        })
}

//客服软件初始化
var Service = function () {
    this.init();
}
Service.prototype.init = function () {
    this.ntkfInit();
}
Service.prototype.ntkfInit = function () {
  function GetUserName() {
    try{
      if (typeof ($.cookie("__hs_uuinfo")) == "undefined") {
        return "";
      }
      else {
        return $.cookie("uuid");
      }
    } catch (e) { }
  }

  function GetUserID() {
    try{
      if (typeof ($.cookie("__hs_uuinfo")) == "undefined") {
        return "";
      }
      else {
        return $.cookie("uuid");
      }
    } catch (e) { }
  }
    window.NTKF_PARAM = {
        siteid: "9141",   //企业ID，必填，取值参见文档开始表
        settingid: "kf_9141_1451977523930",   //缺省客服配置ID，必填，取值参见文档开始表
        uid: GetUserID(),   //用户ID,未登录可以为空
        uname: GetUserName(),   //用户名，未登录可以为空
        isvip: "0",          //是否为vip用户
        userlevel: "普通会员"   //网站自定义会员级别
    }
    if (!NTKF_PARAM) {
        NTKF_PARAM = {};
    }
    if (!NTKF_PARAM.ntalkerparam) {
        NTKF_PARAM.ntalkerparam = {};
    }
    if (!!window.pageInfo && !!window.pageInfo.sku && !!window.pageInfo.sku.skuid) {
        NTKF_PARAM.ntalkerparam.item = {id: window.pageInfo.sku.skuid};
    }
}

//侧边栏初始化
var Right = function () {
    this.rightTipsHtml = null;
    this.openState = false;
    this.init();
}
Right.prototype.init = function () {
    this.rightInit();
    this.listen();
    this.trigger();
    this.rightTipsInit();
    this.rightGuideInit();
}
//右侧初始化兼容浏览器视窗过小
Right.prototype.rightInit = function(){
    //if($(window).height() < (290 + 470)){
    //    $("#right").css({"top":250});
    //}
    var xthis = this;
    $("body>*,html>*").click(function(event){
       if(!$.contains($("#right")[0],event.target)){
           xthis.close();
       }
    })
}
//右侧提示窗口
Right.prototype.rightTipsInit = function () {
    this.rightTipsHtml = '<em class="com-tips-r-bg">◆</em><em class="com-tips-r">◆</em><div class="com-tips-cont right-tips-cont"><div class="right-tips-close">x</div> </div>';
}
Right.prototype.tips = function (html) {
    if(!$.cookie("rightTipsClosed")){
        var xthis = this;
        var newDom = $('<div class="right-tips-box p_relative" style="display: none;"></div>');
        newDom.html(this.rightTipsHtml).find('.right-tips-cont').append(html);
        $("#right").append(newDom);
        newDom.show().addClass("active");
        newDom.find(".right-tips-close").click(function () {
            $.cookie("rightTipsClosed",'true');
            $(this).parents(".right-tips-box").remove();
        })
    }

}
//右侧开启
Right.prototype.open = function () {
    this.openState = true;

    $("#right").animate({'right': '-3px'});

}
//右侧关闭
Right.prototype.close = function () {
    this.openState = false;
    $("#right").animate({'right': '-337px'});
}
/*
右侧滚动栏初始化
传入参数.myCart .myCoupon
*/
Right.prototype.jsScroll = function (lazy) {
    $(".right-cont").find('.rel-cont').each(function(){
        var $this=$(this);
        var parent = $this.parents('.right-cont-item');
       if(parent.css('display')=='block'&& ($(".jscroll-c",$this).length<=0 || !lazy)){
           //保证只初始化一遍
            $this.jscroll({W: "5", Btn: {btn: false}});



       }
    });
}
//右侧回到顶部
Right.prototype.scrollTop = function () {
    if ($('html').scrollTop()) {
        $('html').animate({scrollTop: 0}, 800);
        return false;
    }
    $('body').animate({scrollTop: 0}, 800);
}
//右侧导航事件
Right.prototype.rightGuideInit = function () {
    var xthis = this;
    $("#right").removeClass('r-visiable');
    $(".right-guide").find("li").hover(function () {
        $(this).find("em").stop(false, true);
        $(this).find("em").show(0).animate({'left': '-70px', 'width': '80px'}, 70 * 5);
    }, function () {
        $(this).find("em").stop(false, true);
        $(this).find("em").hide(0).animate({'left': '0px', 'width': '50px'});

    })
    //切换选项卡
    $(".right-guide").find("li").click(function () {
        var index = $(this).index();
        if (index < $(".right-guide").find("li").length - 2) {
            if(TopLogin.IsLogin||(index==1)){
                $('.right-cont-item').eq(index).show().siblings().hide();
                if (!xthis.openState) {
                    xthis.open();

                }
                xthis.jsScroll(true);
            }else{
                $('.right-cont-item').eq(0).show().siblings().hide();
                if (!xthis.openState) {
                    xthis.open();

                }
            }


        }
    })
    //回到最顶部
    $('.guide-icon-7').parents('li').click(function () {
        xthis.scrollTop();
    })
    //右侧关闭
    $(".right-cont").find('.close').click(function () {
        xthis.close();
    });
}
//右侧监听的全局事件
Right.prototype.listen = function () {
    //关闭广告后须知
    //EventLoop.listen('topAdClosed', function (adTop) {
    //    var newtop = parseInt($("#right").css('top')) - adTop;
    //    $('#right').css({'top': newtop});
    //});

}
Right.prototype.trigger = function(){
    if($.cookie("topAdClosed")){
        EventLoop.trigger('topAdClosed',$('.top-banner').height());
    }
}


//首页氛围加载
$(function () {
  var startTime="2017/3/10 20:00",endTime="2017/3/20 20:00";   //氛围有效时间
  var topImg = "http://img1.aolaigo.com/group1/M00/19/05/CggUF1i5HcWAOYj7AAD1diHRiEk591.png";  //公共头部背景
  var midImg = "http://img1.aolaigo.com/group1/M00/19/08/CggUF1i5KbqAQBKnAAFqLCRmV-U825.png";  //首页轮播背景
  var lowerImg = "http://img1.aolaigo.com/group1/M00/19/08/CggUF1i5KbqASOF7AACgo0Vtexo194.png";//首页轮播下方背景
  var floorImgs = [   //首页逐楼层循环放置背景
    "http://img1.aolaigo.com/group1/M00/19/0C/CggUF1i5N5-AYq9yAAAdnu-jeYM630.png",
    "http://img1.aolaigo.com/group1/M00/19/11/CggUFli6FSeAQ-3GAAAeUoKYZWw273.png",
    "http://img1.aolaigo.com/group1/M00/19/11/CggUFli6FSeAbnjVAAAdneFdR-g269.png",
    "http://img1.aolaigo.com/group1/M00/19/11/CggUFli6FSeAQ-3GAAAeUoKYZWw273.png"
  ];
  startTime = +new Date(startTime);
  endTime = +new Date(endTime);

  function loadTheme() {
    var topBan=$(".top-main.w_full");
    var midBan=$("#indbig.focusv3");
    var lowerBan=$(".Stick.w_full");
    topBan.css(getBkgStyle(topImg));
    midBan.css(getBkgStyle(midImg));
    lowerBan.css(getBkgStyle(lowerImg));

    $(".Seckill.w_full,.w_full.floor").each(function (i) {
      $(this).css(getBkgStyle(floorImgs[i%floorImgs.length]));
    });

    function getBkgStyle(img) {
      return {
        "background-image":"url("+img+")",
        "background-repeat":"no-repeat",
        "background-position":"center top"
      };
    }
  }

  function checkTime(){
    var nowTime = /*new Date("2017/3/18 20:00");//*/+new Date();
    if(startTime<=nowTime && nowTime<endTime) waitForPublicDOM();
  }

  //等待公共部分异步加载完毕后加载氛围
  function waitForPublicDOM() {
    if($(".top-main.w_full").length>0){
      loadTheme();
      return;
    }
    setTimeout(waitForPublicDOM,200);
  }

  checkTime();
});




//暴露单列

var topSignal = getSignal(function (option) {
    new Top(option);
    return true;
});
var footSignal = getSignal(function () {
    new Foot();
    return true;
});

var serviceSignal = getSignal(function () {
    new Service();
    return true;
});
var rightSignal = getSignal(function () {
    var right = new Right();
    EventLoop.trigger('rightObjectUsed', right);
    return right;
});

$(function(){
    $('.categoryBoxTabUl').append('<li data-category="allbrand">全部品牌</li>')
    var allbrandhtmlstr='<div class="allbrand"><div class="categorySmallBox"><div class="categoryBoxList" id="a-z"></div></div>\
        <div class="categorySmallBox"><p class="categoryBoxTitle"><span>品牌</span></p>\
        <div class="categoryBoxList" id="allbrand"><div class="brand-container"></div>\
        <div class="clearfloat"></div><div class="idx-pages"><span class="idx-page selected">1</span></div></div></div></div>'
    $('.categoryBoxTabList').append(allbrandhtmlstr)
    var unicodePinyinDict = "";
    function getFirstLetter(str) {
        if (typeof str !== "string") str = str + '';
        //获取首个字符unicode
        var firstChar = str.charAt(0);
        var unicode = firstChar.charCodeAt(0);
        //检查该unicode码是否在处理范围之内,在则返回该码对映汉字的拼音首字母,不在则返回首个字符
        if (unicode > 40869 || unicode < 19968) {
            if ( unicode>=65&&unicode<=90||unicode>=97&&unicode<=122) return firstChar.toUpperCase();
            else return "#";
        } else {
            return unicodePinyinDict.charAt(unicode - 19968)
        }
    }


    function getBrandsList(){
        var cachedDataJson = localStorage.getItem("cacheAllBrandsList");
        var cachedData = cachedDataJson ? JSON.parse(cachedDataJson) : null;
        if(cachedData && cachedData.expires>Date.now()){
            renderAllBrandsList(cachedData)
        }else {
            $.ajax({
                url: 'http://searchapi.aolaigo.com/appsearch/cmd/GetBrands?cmd=GetBrands',
                dataType: "json",
                success: function (res) {
                    if (res.Code=='0') {
                        //设置缓存时效
                        var expireDate = new Date();
                        expireDate.setHours(expireDate.getHours()+1); //1小时后失效
                        res.Data.expires=+expireDate;
                        localStorage.setItem("cacheAllBrandsList" +
                            "",JSON.stringify(res.Data.MainVM.Sbvms));
                        //console.log(res.Data);
                        renderAllBrandsList(res.Data.MainVM.Sbvms)
                    }
                },
                error: function (res) {
                    if(cachedData) renderAllBrandsList(cachedData);
                    console.log(res);
                }
            });
        }
    }
    getBrandsList();


    function renderAllBrandsList(Data){
        var indexedBrands=[];
        var indexedLetter=[];
        $.each(Data,function (i) {
            var pinyin = getFirstLetter(this.bname);
            if(!indexedBrands[pinyin]) {
                indexedBrands[pinyin]=[];
                indexedLetter.push(pinyin);
            }
            indexedBrands[pinyin].push(this.bname);
        });
        indexedLetter.sort()
        if (indexedLetter[0]=='#')indexedLetter=indexedLetter.slice(1).concat('#')
        for(var i=0;i<indexedLetter.length;i++){
            $('#a-z').append('<div class="char">'+indexedLetter[i]+'</div>');
        }
        $('.char').click(function(){
            $('.char.selected').removeClass('selected')
            $(this).addClass('selected')
            renderBrandsList(indexedBrands[$(this).html()],1)
        })
        $('.idx-pages').on('click','.idx-page',function(){
            $('.idx-page.selected').removeClass('selected')
            $(this).addClass('selected')
            renderBrandsList(indexedBrands[$('.char.selected').html()])
        })
        $('.brand-container').on('click','.brand-item',function(){
            var key=$(this).html()
            location.href='http://search.aolaigo.com/search.aspx?keyword='+key
        })
        $('.char').eq(0).click()
    }

    function renderBrandsList(data,n){
        var pagesize=30;
        var brandlength=data.length
        var pagelength=Math.ceil(brandlength/pagesize)
        if (n){
            $('.idx-pages').html('')
            for (var i=0;i<pagelength;i++){
                $('.idx-pages').append('<span class="idx-page">'+(i+1)+'</span>')
            }
            $('.idx-pages .idx-page').eq(0).addClass('selected')
        }
        var page=$('.idx-page.selected').html();
        var html=''
        $('.brand-container').html('');
        for (var i=(page-1)*pagesize;i<page*pagesize&&i<brandlength;i++){
            html+='<div class="brand-item">'+data[i]+'</div>'
        }
        $('.brand-container').html(html);
    }
})

