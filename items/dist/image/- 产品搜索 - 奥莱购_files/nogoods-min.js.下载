$(document).ready(function(){setNoFindProducts()});function setNoFindProducts(){$.ajax({cache:true,url:hs.domain.search+"/cmssearch/cmd/GetCmsSearchResult",data:"cmd=GetCmsSearchResult&keyword=*&order=5_1&page=1&psize=4",async:true,type:"GET",dataType:"json",error:function(a,c,b){},success:function(l){var g="";g+='<div class="search-hot-t">\u70ed\u9500\u5546\u54c1\u63a8\u8350</div>';g+="<ul>";if(l.Data&&l.Data.MainOutputVM&&l.Data.MainOutputVM.OutputSkuInfoVMs){for(var b=0;b<l.Data.MainOutputVM.OutputSkuInfoVMs.length;b++){try{var k=l.Data.MainOutputVM.OutputSkuInfoVMs[b];var c=hs.domain.item+"/product/"+k.wcode+".html";var h="";h+='<li><div id="pro-z" class="p1"><dl><dd style="display:block;"><a href="'+c+'">';var j=k.wmimg.url;var a=k.wmimg.url;if(j){var f=j.lastIndexOf(".");if(f<0){f=0}if(j.indexOf("http://")<0){j=hs.domain.getimg()+"/group1/"+j.substring(0,f)+"=220x220"+j.substring(f);a=hs.domain.getimg()+"/group1/"+a.substring(0,f)+"=35x35"+a.substring(f)}else{j=j.substring(0,f)+"=220x220"+j.substring(f);a=hs.domain.getimg()+"/group1/"+a.substring(0,f)+"=35x35"+a.substring(f)}}h+='<img src="'+j+'"/></a></dd></dl></div>';h+='<div id="xt" class="p2"><div class="src-div-thumbnail">';h+='<dl class="src-dl-thumbnail">';h+='<dd class="">';h+='<img src="'+a+'" style="opacity: 1;" loaded="1">';h+="</dd>";h+="</dl>";h+="</div></div>";h+='<div style="clear: both"></div>';h+='<div class="p3"><a target="_blank" href="'+c+'">'+k.wname+"</a></div>";if(k.actprice==0){h+='<div class="p4"><span></span><span class="p4-jg01">\u00a5'+k.sprice+"</span><del>\u00a5"+k.mprice+"</del></div>"}else{h+='<div class="p4"><span></span><span class="p4-jg01">\u00a5'+k.actprice+"</span><del>\u00a5"+k.mprice+"</del></div>"}h+="</li>";g+=h}catch(d){}}}g+="</ul>";$(g).appendTo($(".search-hot"))}})};
